{
    "env": {
        "browser": false,
        "node": false,
        "amd": false
    },

    "rules": {
        "no-alert": 1,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-console": 1,
        "no-comma-dangle": 1,
        "no-debugger": 1,
        "no-dupe-keys": 1,
        "no-empty": 1,
        "no-empty-class": 1,
        "no-eval": 1,
        "no-ex-assign": 1,
        "no-func-assign": 1,
        "no-implied-eval": 1,
        "no-with": 1,
        "no-fallthrough": 1,
        "no-unreachable": 1,
        "no-undef": 1,
        "no-undef-init": 1,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-obj-calls": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-native-reassign": 1,
        "no-delete-var": 1,
        "no-return-assign": 1,
        "no-new-array": 1,
        "no-new-object": 1,
        "no-label-var": 1,
        "no-underscore-dangle": 1,
        "no-loop-func": 1,
        "no-empty-label": 1,
        "no-unused-vars": 1,
        "no-script-url": 1,
        "no-proto": 1,
        "no-iterator": 1,
        "no-wrap-func": 1,
        "no-shadow": 1,
        "no-use-before-define": 1,
        "no-redeclare": 1,
        "camelcase": 1,
        "curly": 1,
        "dot-notation": 1,
        "eqeqeq": 1,
        "new-parens": 1,
        "new-cap": 1,
        "semi": 1,
        "use-isnan": 1,
        "quotes": 1,
        "regex-spaces": 1,
        "no-multi-str": 1,
        "unnecessary-strict": 1
    },

    "ruleConfigs": {
        "quotes": ["double"]
    }
}

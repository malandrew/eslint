{
    "env": {
        "browser": false,
        "node": false,
        "amd": false
    },

    "rules": {
        "no-alert": "*",
        "no-caller": "*",
        "no-catch-shadow": "*",
        "no-console": "*",
        "no-comma-dangle": "*",
        "no-debugger": "*",
        "no-dupe-keys": "*",
        "no-empty": "*",
        "no-empty-class": "*",
        "no-eval": "*",
        "no-ex-assign": "*",
        "no-func-assign": "*",
        "no-implied-eval": "*",
        "no-with": "*",
        "no-fallthrough": "*",
        "no-unreachable": "*",
        "no-undef": "*",
        "no-undef-init": "*",
        "no-octal": "*",
        "no-octal-escape": "*",
        "no-obj-calls": "*",
        "no-new-wrappers": "*",
        "no-new": "*",
        "no-new-func": "*",
        "no-native-reassign": "*",
        "no-delete-var": "*",
        "no-return-assign": "*",
        "no-new-array": "*",
        "no-new-object": "*",
        "no-label-var": "*",
        "no-underscore-dangle": "*",
        "no-loop-func": "*",
        "no-empty-label": "*",
        "no-unused-vars": "*",
        "no-script-url": "*",
        "no-proto": "*",
        "no-iterator": "*",
        "no-wrap-func": "*",
        "no-shadow": "*",
        "no-use-before-define": "*",
        "no-redeclare": "*",
        "camelcase": "*",
        "curly": "*",
        "dot-notation": "*",
        "eqeqeq": "*",
        "new-parens": "*",
        "new-cap": "*",
        "semi": "*",
        "use-isnan": "*",
        "quotes": "*",
        "regex-spaces": "*",
        "no-multi-str": "*",
        "unnecessary-strict": "*"
    },

    "ruleConfigs": {
        "quotes": ["double"]
    },

    "ruleLogLevels": {

    }
}
